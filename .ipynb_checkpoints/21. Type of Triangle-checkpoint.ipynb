   "source": [
    "SELECT\n",
    "    CASE\n",
    "        WHEN 2 * GREATEST(A, B, C) >= (A+B+C) THEN 'Not A Triangle'\n",
    "        WHEN A=B AND B=C THEN 'Equilateral'\n",
    "        WHEN A=B OR A=C OR B=C THEN 'Isosceles'\n",
    "        ELSE 'Scalene'\n",
    "    END\n",
    "FROM triangles;"
   ]
  }
