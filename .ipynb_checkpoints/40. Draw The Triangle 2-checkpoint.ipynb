   "source": [
    "DELIMITER //\n",
    "\n",
    "CREATE PROCEDURE P(n INT)\n",
    "BEGIN\n",
    "    DECLARE i INT;\n",
    "    SET i = 1;\n",
    "    WHILE i <= n DO\n",
    "        SELECT REPEAT('* ', i);\n",
    "        SET i = i + 1;\n",
    "    END WHILE;\n",
    "END //\n",
    "\n",
    "DELIMITER ;\n",
    "\n",
    "CALL P(20);"
   ]
  }
